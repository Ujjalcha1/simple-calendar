{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": [
            "dom",
            "esnext"
        ],
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}